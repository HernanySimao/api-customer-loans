generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Client {
    id       Int      @id @default(autoincrement())
    age      Int
    cpf      String   @unique @db.VarChar(255)
    name     String
    income   Int      @default(0)
    location String   @db.VarChar(100)
    createAt DateTime @default(now())

    @@map("Client")
}
